= content_for :body do
  %div.year_list.pagination
    %ul
      - @years.each do |year|
        %li= link_to year, refinery.publications_publications_path(year: year)
  %div#publications
    - @publications.each do |publication|
      %p.publication
        = publication.authors
        %br
        %strong= publication.title
        %br
        = publication.review

= render '/refinery/content_page'
